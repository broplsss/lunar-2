                            (D === _ && G <= l || D !== _ && G <= 0) && R++
                        }
                        if (3 <= R) {
                            if (h) return !0;
                            var N = this.createContactEquation(E, O, S, k);
                            v++;
                            F = S.vertices[_ % S.vertices.length], U = S.vertices[(_ + 1) % S.vertices.length];
