                    n = s - e.halfHeight;
                return o * o + n * n <= t.radius * t.radius
            }, PIXI.Circle = H.Circle, H.Ellipse = function(t, e, i, s) { t = t || 0, e = e || 0, i = i || 0, s = s || 0, this.x = t, this.y = e, this.width = i, this.height = s, this.type = H.ELLIPSE }, H.Ellipse.prototype = {
