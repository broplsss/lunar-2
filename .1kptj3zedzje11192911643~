                coordinatesOnLine: function(t, e) {
                    void 0 === t && (t = 1), void 0 === e && (e = []);
                    var i = Math.round(this.start.x),
                        s = Math.round(this.start.y),
                        o = Math.round(this.end.x),
                        n = Math.round(this.end.y),
                        a = Math.abs(o - i),
