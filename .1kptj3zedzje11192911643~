                        s = Math.max(this.start.x, this.end.x),
                        o = Math.min(this.start.y, this.end.y),
                        n = Math.max(this.start.y, this.end.y);
                    return this.pointOnLine(t, e) && i <= t && t <= s && o <= e && e <= n
                },
